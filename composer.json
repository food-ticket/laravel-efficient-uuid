{
    "name": "foodticket/laravel-efficient-uuid",
    "type": "utility",
    "description": "A package to extend Laravel migrations adding a more efficient storage of UUID fields in your database",
    "license": "MIT",
    "authors": [
        {
            "name": "Henrik Steffensen",
            "email": "henrik@foodticket.nl",
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2",
        "dyrynda/laravel-model-uuid": "^7.0 || 8.0.0-beta.1",
        "illuminate/container": "^10.0 || ^11.0",
        "illuminate/contracts": "^10.0 || ^11.0",
        "illuminate/database": "^10.0 || ^11.0",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.3 || ^1.4",
        "mockery/mockery": "^1.4.4 || ^1.5 || ^1.6",
        "orchestra/testbench": "^8.0 || ^9.0",
        "phpunit/phpunit": "^9.6.0 || ^10.0.7 || ^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "Dyrynda\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "5.x-dev"
        }
    }
}
